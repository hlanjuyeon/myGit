<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.globalin.shine.mapper.orgchart.OrgchartMapper">
    <select id="getEmpListByDept" resultType="employeeVO">
        SELECT * FROM employee WHERE DeptName = #{deptName}
    </select>
    <select id="getEmp" resultType="employeeVO">
        SELECT * FROM employee WHERE no = #{no}
    </select>
</mapper>